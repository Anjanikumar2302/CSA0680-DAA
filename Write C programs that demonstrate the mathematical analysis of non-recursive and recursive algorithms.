#include <stdio.h>
int factorialNonRecursive(int n) {
    int result = 1;
    for (int i = 1; i <= n; i++) {
        result *= i;
    }
    return result;
}
int main() {
    int num = 5;
    int result = factorialNonRecursive(num);
    printf("Factorial of %d is %d\n", num, result);
    return 0;
}
